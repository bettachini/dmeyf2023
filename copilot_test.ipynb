   "source": [
    "# quiero un array de los primeros 100 numeros primos\n",
    "import numpy as np\n",
    "\n",
    "\n",
    "def es_primo(n):\n",
    "    if n < 2:\n",
    "        return False\n",
    "    for i in range(2, n):\n",
    "        if n % i == 0:\n",
    "            return False\n",
    "    return True\n",
    "\n",
    "arreglo = np.array([i for i in range(100) if es_primo(i)])\n",
    "\n",
    "arreglo"
   ]
